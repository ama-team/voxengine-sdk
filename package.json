{
  "name": "@ama-team/voxengine-sdk",
  "version": "0.1.0",
  "description": "Bundle of helpers for VoxEngine scenario scripts",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "test:coverage:report": "node_modules/.bin/istanbul cover _mocha",
    "test:coverage": "npm run test:coverage:report && npm run coveralls",
    "coveralls": "cat report/coverage/lcov.info | node_modules/.bin/coveralls",
    "test:ignore": "npm run test || true",
    "test:report": "npm run allure:clean && npm run test:ignore && npm run allure:report",
    "allure:clean": "rm -rf report/allure-results report/allure",
    "allure:report": "allure generate -o report/allure -- report/allure-results",
    "allure:open": "allure report open -o report/allure"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ama-team/voxengine-sdk.git"
  },
  "keywords": [
    "voximplant"
  ],
  "author": "AMA Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ama-team/voxengine-sdk/issues"
  },
  "homepage": "https://github.com/ama-team/voxengine-sdk#readme",
  "dependencies": {
    "@ama-team/voxengine-definitions": "^0.1.0",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-string": "^1.3.0",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mocha-allure-reporter": "^1.2.4",
    "mocha-multi": "^0.9.1",
    "mocha-multi-reporters": "^1.1.1",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2"
  }
}
